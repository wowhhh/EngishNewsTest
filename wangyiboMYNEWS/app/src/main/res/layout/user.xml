<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <RelativeLayout
        android:id="@+id/fragment_dynamic_one"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#fff"
        android:layout_marginTop="10dp"
        >
        <ImageView
            android:id="@+id/iv_friend"
            android:src="@drawable/strawberry"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:scaleType="fitXY"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"

            />
        <Button
            android:id="@+id/button_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/iv_friend"
            android:layout_marginLeft="5dp"
            android:text="登录"
            style="?android:attr/borderlessButtonStyle"
             />
        <ImageView
            android:layout_marginRight="10dp"
            android:id="@+id/iv_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/strawberry"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/devide_line"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="24dp"
        android:background="@color/devide_line"/>
    <LinearLayout

        android:id="@+id/fragment_dynamic_two"
        android:background="#fff"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:id="@+id/layout_xiangce"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >
            <ImageView
                android:id="@+id/iv_xiangce"
                android:src="@drawable/strawberry"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"

                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_xiangce"
                android:layout_marginLeft="5dp"
                android:id="@+id/button_collect"
                style="?android:attr/borderlessButtonStyle"
                android:text="我的收藏" />
            <ImageView
                android:layout_marginRight="10dp"
                android:id="@+id/iv_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/strawberry"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#CBCED2" />
        <RelativeLayout

            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >
            <ImageView
                android:id="@+id/iv_activity"
                android:src="@drawable/strawberry"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"

                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_activity"
                android:layout_marginLeft="5dp"
                android:id="@+id/button_history"
                style="?android:attr/borderlessButtonStyle"
                android:text="浏览历史" />
            <ImageView
                android:layout_marginRight="10dp"
                android:id="@+id/iv_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/strawberry"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#CBCED2" />
        <RelativeLayout

            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >
            <ImageView

                android:id="@+id/iv_folder"
                android:src="@drawable/strawberry"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"

                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_folder"
                android:layout_marginLeft="5dp"
                android:id="@+id/share_history"
                style="?android:attr/borderlessButtonStyle"
                android:text="分享历史" />
            <ImageView
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/strawberry"/>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#CBCED2" />
        <RelativeLayout

            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >
            <ImageView
                android:id="@+id/iv_neighbour"
                android:src="@drawable/strawberry"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"

                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_neighbour"
                android:layout_marginLeft="5dp"
                android:id="@+id/creatTable"
                style="?android:attr/borderlessButtonStyle"
                android:text="一键创表！" />
            <ImageView
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/strawberry"/>
        </RelativeLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/devide_line"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="24dp"
        android:background="@color/devide_line"/>
    <LinearLayout
        android:id="@+id/fragment_dynamic_three"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#fff"
        android:orientation="vertical"
        >

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#CBCED2" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >
            <ImageView
                android:id="@+id/iv_qun"
                android:src="@drawable/strawberry"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"

                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_qun"
                android:layout_marginLeft="5dp"
                android:enabled="false"
                android:id="@+id/button_loginout"
                style="?android:attr/borderlessButtonStyle"
                android:text="退出登录" />
            <ImageView
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/strawberry"/>
        </RelativeLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#CBCED2" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            >
            <ImageView
                android:id="@+id/iv_qun"
                android:src="@drawable/strawberry"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"

                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/iv_qun"
                android:layout_marginLeft="5dp"
                android:enabled="false"
                android:id="@+id/my_back"
                style="?android:attr/borderlessButtonStyle"
                android:text="用户反馈" />
            <ImageView
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/strawberry"/>
        </RelativeLayout>
    </LinearLayout>


</LinearLayout>